{
  "rules": {
    "react/prop-types": 0
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["next/core-web-vitals", "plugin:react/recommended", "prettier","plugin:react/jsx-runtime"],
  "plugins": ["react", "prettier"]
}
